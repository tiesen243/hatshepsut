{
  "name": "tiesen/hatshepsut",
  "version": "1.0.0",
  "license": "MIT",
  "type": "project",
  "scripts": {
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "MODE=development npx concurrently -c=\"#FF2D20,#646CFF\" \"php -S 0.0.0.0:8000 -t public\" \"npm run dev\" --names=server,vite --kill-others"
    ],
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "MODE=production php -S 0.0.0.0:8000 -t public"
    ]
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Framework\\": "src/"
    }
  }
}
